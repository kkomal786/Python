    "## WAP to check whether the year entered is an leap year or not"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a year2000\n",
      "2000 is a leap year\n"
     ]
    }
   ],
   "source": [
    "c = int(input(\"Enter a year\"))\n",
    "if(c % 4 == 0):\n",
    "    print(c,\"is a leap year\")\n",
    "else:\n",
    "    print(c, \"is not a leap year\")"
   ]
  }
